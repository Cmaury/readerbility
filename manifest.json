{
  "name": "ReaderBility",
  "version": "0.0.1",
  "description": "Make Google Reader more accessible to the Visually Impaired",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
  "tabs", "http://google.com/reader/*"
  ],
  "content_scripts":[
     {
	"matches": [http://www.google.com/reader/*"],
	"css": ["readerbility.css"]
	"js": ["readerbility.js"],
     }
}
